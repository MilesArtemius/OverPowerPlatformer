  <items>
      <VBox>
         <children>
             <Text fx:id="text1" textAlignment="CENTER">
               <VBox.margin>
                  <Insets bottom="10.0" />
               </VBox.margin></Text>
             <Accordion fx:id="ap1" />
         </children>
      </VBox>
      <VBox>
         <children>
             <Text fx:id="text2" textAlignment="CENTER">
               <VBox.margin>
                  <Insets bottom="10.0" />
               </VBox.margin></Text>
             <Accordion fx:id="ap2" minHeight="0.0" minWidth="0.0" prefHeight="160.0" prefWidth="100.0" />
         </children>
      </VBox>
  </items>
</SplitPane>
