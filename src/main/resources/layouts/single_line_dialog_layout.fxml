<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<DialogPane prefHeight="200.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="classes.LevelEditor.SinglelineDialigController">
   <header>
      <TextArea editable="false" prefHeight="50" prefWidth="Infinity" text="Header">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </TextArea>
   </header>
   <content>
      <VBox prefHeight="200.0" prefWidth="100.0">
         <children>
            <TextField fx:id="textField" promptText="First level name..." />
            <Region prefHeight="200.0" prefWidth="200.0" />
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Region prefHeight="100.0" prefWidth="100.0" />
                  <Button mnemonicParsing="false" text="OK" />
                  <Region prefHeight="100.0" prefWidth="100.0" />
                  <Button mnemonicParsing="false" text="Cancel" />
                  <Region prefHeight="100.0" prefWidth="100.0" />
               </children>
            </HBox>
         </children>
      </VBox>
   </content>
</DialogPane>
